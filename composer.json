{
  "require": {
    "craftcms/cms": "3.1.4",
    "vlucas/phpdotenv": "^2.4.0",
    "lukeyouell/craft-queue-manager": "1.1.0",
    "aelvan/imager": "v2.1.4",
    "craftcms/aws-s3": "1.0.8"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
