{% set entryQuery = craft.entries().section('author').site('*') %}
{# Execute the query and get the results #}
{% set entries = entryQuery.all() %}

<ul>
    {% for entry in entries %}
        <li>
            <a href="/{{entry.uri}}">{{entry.title}}</a>
        </li>


    {% endfor %}
</ul>
